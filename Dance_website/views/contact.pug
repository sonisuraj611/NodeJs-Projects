extends base.pug

//- block scripts
block style 
    style
        include ../static/style.css
        include ../static/stylecontact.css
block content
    div.container
      h3 Contact Form
    form(action="/contact", method="post",class="formclass") 
        input(type="text",class="input",name="name",placeholder="Enter Your Name")
        input(type="number",class="input",name="age",placeholder="Enter Your Age")
        input(type="text",class="input",name="gender",placeholder="Enter Your Gender")
        input(type="text",class="input",name="address",placeholder="Enter Your Address")
        input(type="text",class="inputAdd",name="more",placeholder="More About Yourself")
        button(class="btn")
          | Submit